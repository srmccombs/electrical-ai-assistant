# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Supabase Service Role Key (for server-side operations)
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# API Security (for production)
API_SECRET_KEY=generate_a_secure_random_string_here

# CORS Configuration (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Optional: Environment
NODE_ENV=development

# Optional: Cache Settings
AI_CACHE_TTL=3600000  # 1 hour in milliseconds
SEARCH_RATE_LIMIT=100  # requests per minute

# Optional: Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Decision Engine Configuration
# Options: disabled (default), shadow, production
USE_DECISION_ENGINE=disabled

# Rollout Configuration (for progressive rollout)
# Percentage of users to use new engine (0-100)
ROLLOUT_PERCENTAGE=0

